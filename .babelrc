{
  "env": {
    "node" : {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": "current"
            }
          }
        ],
        "react",
        "stage-0"
      ],
      "plugins": ["dynamic-import-node", "react-loadable/babel"]
    },
    "browser" : {
      "presets" : [
        [
          "env",
          {
            "targets": {
              "browsers": [
                "last 2 versions"
              ]
            }
          }
        ],
        "react",
        "stage-0"
      ],
        "plugins": ["react-loadable/babel"]
    }
  }
}
